  "license": "ISC",
  "engines": {
    "node": "18.20.4"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "knex": "^3.1.0",
    "meow": "^14.0.0",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/node": "^24.8.1",
    "ts-node": "^10.9",
    "ts-node-dev": "^2.0",
    "typescript": "^5.6"
  }
